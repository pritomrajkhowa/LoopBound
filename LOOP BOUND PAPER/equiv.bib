@article{Gosper1,
	ISSN = {00278424},
	URL = {http://www.jstor.org/stable/67570},
	abstract = {Given a summand an, we seek the ``indefinite sum'' S(n) determined (within an additive constant) by $\underset n=1\to{\overset m\to{\sum}}$an = S(m) - S(0) [0] or, equivalently, by an = S(n) - S(n - 1). [1] An algorithm is exhibited which, given an, finds those S(n) with the property S(n)/S(n-1) = a rational function of n. [2] With this algorithm, we can determine, for example, the three identities $\underset n=1\to{\overset m\to{\sum}}\frac{\underset j=1\to{\overset n-1\to{\prod}}bj^{2}+cj+d}{\underset j=1\to{\overset n\to{\prod}}bj^{2}+cj+e}$ = $\frac{1-\underset j=1\to{\overset m\to{\prod}}\frac{bj^{2}+cj+d}{bj^{2}+cj+e}}{e-d}$, [3a] $\underset n=1\to{\overset m\to{\sum}}\frac{\underset j=1\to{\overset n-1\to{\prod}}aj^{3}+bj^{2}+cj+d}{\underset j=1\to{\overset n\to{\prod}}aj^{3}+bj^{2}+cj+e}$ = $\frac{1-\underset j=1\to{\overset m\to{\prod}}\frac{aj^{3}+bj^{2}+cj+d}{aj^{3}+bj^{2}+cj+e}}{e-d}$, [3b] and $\underset n=1\to{\overset m\to{\sum}}\frac{\underset j=1\to{\overset n-1\to{\prod}}bj^{2}+cj+d}{\underset j=1\to{\overset n+1\to{\prod}}bj^{2}+cj+e}$ = $\frac{\frac{2b}{e-d}-\frac{3b+c+d-e}{b+c+e}-\left(\frac{2b}{e-d}-\frac{b(2m+3)+c+d-e}{b(m+1)^{2}+c(m+1)+e}\right)\underset j=1\to{\overset m\to{\prod}}\frac{bj^{2}+cj+d}{bj^{2}+cj+e}}{b^{2}-c^{2}+d^{2}+e^{2}+2bd-2de+2eb}$ [3c] and we can also conclude that $\underset n=1\to{\overset m\to{\sum}}\frac{\underset j=1\to{\overset n-1\to{\prod}}j^{3}}{\underset j=1\to{\overset n+1\to{\prod}}j^{3}+1}$ [3d] is inexpressible as S(m) - S(0), for any S(n) satisfying Eq. 2.},
	author = {R. William Gosper},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	number = {1},
	pages = {40--42},
	publisher = {National Academy of Sciences},
	title = {Decision Procedure for Indefinite Hypergeometric Summation},
	volume = {75},
	year = {1978}
}
